{
  "manifest_version": 3,
  "name": "SomBox Scraper",
  "version": "1.1",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["https://leerling.somtoday.nl/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "host_permissions": ["https://leerling.somtoday.nl/*"]
}
